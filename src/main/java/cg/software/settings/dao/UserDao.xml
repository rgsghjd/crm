<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cg.software.settings.dao.UserDao">
<select id="login" resultType="cg.software.settings.domain.User">
    select * from tbl_user where loginact=#{UserAct} and loginpwd=#{UserPwd}
</select>
<select id="getuserList" resultType="cg.software.settings.domain.User">
    select * from tbl_user 
</select>
</mapper>