<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cg.software.workbench.dao.ClueActivityRelationDao">
    <select id="find" resultType="ClueActivityRelation">
        select * from tbl_clue_activity_relation where clueid=#{id}
    </select>

    <delete id="unBun">
        delete from tbl_clue_activity_relation
        where id=#{id}
    </delete>
<delete id="delete">
    delete from tbl_clue_activity_relation where clueid=#{id}
</delete>
  	
</mapper>

































